{
  "name": "kindling",
  "version": "1.0.0",
  "description": "The Kindling static site framework",
  "main": ".eleventy.js",
  "scripts": {
    "build": "webpack --env production && eleventy",
    "compile": "npx eleventy",
    "start": "npm run devserve",
    "devserve": "NODE_ENV=development npx babel-node --presets @babel/preset-env ./serve.js",
    "spritesheet": "node ./svgsprite.js",
    "modernizr": "npx modernizr -c .modernizrrc.json -d src/js/modernizr.js -u",
    "generate": "npm run modernizr && npm run spritesheet && npm run build"
  },
  "author": "Matthew Gaffen",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.0.6",
    "bluebird": "^3.7.2",
    "browser-sync": "^2.26.3",
    "bs-fullscreen-message": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "co": "^4.6.0",
    "compression": "^1.7.4",
    "css-loader": "^5.0.0",
    "debug": "^4.1.1",
    "eleventy-plugin-yamldata": "^0.1.1",
    "file-loader": "^6.1.1",
    "jimp": "^0.16.1",
    "lodash.debounce": "^4.0.8",
    "markdown-it": "^12.0.2",
    "markdown-it-link-attributes": "^3.0.0",
    "mini-css-extract-plugin": "^1.2.1",
    "modernizr": "^3.11.3",
    "node-sass": "^7.0.0",
    "nunjucks-date": "^1.4.0",
    "postcss": "^8.1.4",
    "postcss-loader": "^4.0.4",
    "sass-loader": "^10.0.3",
    "strip-ansi": "^6.0.0",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.0",
    "svgstore": "^3.0.0-2",
    "terser-webpack-plugin": "^5.0.3",
    "tree-kill": "^1.2.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {}
}
